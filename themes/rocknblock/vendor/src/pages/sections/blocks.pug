mixin webpImg(loading, src, type, alt, useclass)
  picture(class=useclass)
    source(type="image/webp", srcset=src+'.webp')
    if type === '-'
      img(loading=loading, src=src, alt=alt)
    else
      img(loading=loading, src=src+'.'+type, alt=alt)

mixin block-title(title, titleText)
    .block-header
      h2.primary-header= title
      if titleText
        h3.secondary-header= titleText
      if block
        block

