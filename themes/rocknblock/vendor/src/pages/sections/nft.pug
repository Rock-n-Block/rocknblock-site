mixin why-choose()
  section.why-choose
    .block
      h2.title Why Choose Rock'n'Block for NFT Marketplace Development?
      .secondText Here are your benefits of doing business with us.
      .sliderContainerWrapper
        .leftShadowBlock
        .rightShadowBlock
        .swiper.sliderContainer.swiper-business
          img.redLine(src="../../img/nft/Line2.png" alt="line")
          .swiper-wrapper
            .swiper-slide.blockSlide
              .slideTitle 250+ Projects
              .slideDescription created on-a-turn-key basis
            .swiper-slide.blockSlide
              .slideTitle 1 Bn+ dollar
              .slideDescription capitalization of the projects we have built
            .swiper-slide.blockSlide
              .slideTitle 15 000+
              .slideDescription smart contracts deployed
            .swiper-slide.blockSlide
              .slideTitle 15+ years
              .slideDescription of development experience
            .swiper-slide.blockSlide
              .slideTitle 50+ Team
              .slideDescription members (mostly developers)
