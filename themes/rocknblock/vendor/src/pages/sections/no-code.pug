include blocks

mixin hero
  section.hero
    .container
      .block-head
        h1.title 
          span.text-border No Code 
          | Blockchain Solutions
        p.text Create and Launch Smart Contract without coding, right from your browser.
        a.button(href="#contact-us") Launch app
          img(loading='lazy' src='../../img/icons/icon-arrow-right-mask.svg' alt='arrow right')
      +webpImg('lazy',`../../img/no-code/hero`,'png','No Code Blockchain Solutions','img')

mixin library
  section.library
    .container
      .block-head
        h1.title Smart contracts library:
      .content
        .library-item
          .img
            img(loading='lazy' src=`../../img/no-code/library-token.svg` alt="Token contract")
          h3.title Token contract
        .library-item
          .img
            img(loading='lazy' src=`../../img/no-code/library-crowdsale.svg` alt="Crowdsale Contract")
          h3.title Crowdsale Contract
        .library-item
          .img
            img(loading='lazy' src=`../../img/no-code/library-airdrop.svg` alt="Airdrop Contract")
          h3.title Airdrop Contract
        .library-item
          .img
            img(loading='lazy' src=`../../img/no-code/library-protactor.svg` alt="Token Protector")
          h3.title Token Protector
        .library-item
          .img
            img(loading='lazy' src=`../../img/no-code/library-lost-key.svg` alt="Lost Key Contract")
          h3.title Lost Key Contract
        .library-item
          .img
            img(loading='lazy' src=`../../img/no-code/library-deferrer-payment.svg` alt="Deferred Payment")
          h3.title Deferred Payment 

mixin network
  section.network
    .container
      .block-head
        h1.title Supported networks:
      .content
        .swiper.swiper-networks
          .navigation
            img.network-nav-prev(src="../../img/gamefi/arrow-left.svg" alt="nav left")
            img.network-nav-next(src="../../img/gamefi/arrow-right.svg" alt="nav right")
          .swiper-wrapper
            .swiper-slide.slide
              .network-item.large
                .top
                  h3.title Ethereum 
                  p.text Create and Deploy predesigned smart contracts template to Ethereum Mainnet and Testnet with a few clicks. You can customise and define the parameters, choose contract functions and get 100% ownership of your contract.
                  a.button(href="#contact-us") Discover solutions
                    img(loading='lazy' src='../../img/icons/icon-arrow-right-mask.svg' alt='arrow right')
                .bottom
                  +webpImg('lazy',`../../img/no-code/network-logo-ethereum`,'png','Ethereum','img')
                  span.bottom-title Contracts:
                  .bottom-item
                    img(loading='lazy' src=`../../img/no-code/network-contract-crowdsale.svg` alt="Crowdsale")
                    span.item-title Crowdsale
                  .bottom-item
                    img(loading='lazy' src=`../../img/no-code/network-contract-airdrop.svg` alt="Aidrop")
                    span.item-title Aidrop
                  .bottom-item
                    img(loading='lazy' src=`../../img/no-code/network-contract-lostkey.svg` alt="Lost key")
                    span.item-title Lost key
                  .bottom-item
                    img(loading='lazy' src=`../../img/no-code/network-contract-defferent-payment.svg` alt="Deffered Payment")
                    span.item-title Deffered Payment
                  .bottom-item
                    img(loading='lazy' src=`../../img/no-code/network-contract-investment-pool.svg` alt="Investment pool")
                    span.item-title Investment pool
            .swiper-slide.slide
              .network-item.short
                .top
                  +webpImg('lazy',`../../img/no-code/network-logo-solana`,'png','Solana','img')
                  span.title Solana
                  p.text Create and Manage Solana SPL tokens with MyWish no code solutions
                  a.button(href="#contact-us") Discover solutions
                    img(loading='lazy' src='../../img/icons/icon-arrow-right-mask.svg' alt='arrow right')
              .network-item.short
                .top
                  +webpImg('lazy',`../../img/no-code/network-logo-moonriver`,'png','Moonriver','img')
                  span.title Moonriver
                  p.text Create and Manage Moonriver based ERC20 tokens with MyWish no code solutions
                  a.button(href="#contact-us") Discover solutions
                    img(loading='lazy' src='../../img/icons/icon-arrow-right-mask.svg' alt='arrow right')
            .swiper-slide.slide
              .network-item.short
                .top
                  +webpImg('lazy',`../../img/no-code/network-logo-tron`,'png','Tron','img')
                  span.title Tron
                  p.text Create, Manage, and Distribute TRC20 Polygon based tokens with MyWish no code solutions
                  a.button(href="#contact-us") Discover solutions
                    img(loading='lazy' src='../../img/icons/icon-arrow-right-mask.svg' alt='arrow right')
              .network-item.short
                .top
                  +webpImg('lazy',`../../img/no-code/network-logo-xdc`,'png','XDC','img')
                  span.title XDC
                  p.text Create and Manage XDC based XRC20 tokens with MyWish no code solutions
                  a.button(href="#contact-us") Discover solutions
                    img(loading='lazy' src='../../img/icons/icon-arrow-right-mask.svg' alt='arrow right')
            .swiper-slide.slide
              .network-item.large.left(style="--network-large-color: #EFD12B;")
                .top
                  h3.title BNB Smart Chain
                  p.text Create and Deploy predesigned smart contracts template to BNB Smart Chain Mainnet and Testnet with a few clicks. You can customise and define the parameters, choose contract functions and get 100% ownership of your contract. 
                  a.button(href="#contact-us") Discover solutions
                    img(loading='lazy' src='../../img/icons/icon-arrow-right-mask.svg' alt='arrow right')
                .bottom 
                  +webpImg('lazy',`../../img/no-code/network-logo-bnb`,'png','Binance','img')

