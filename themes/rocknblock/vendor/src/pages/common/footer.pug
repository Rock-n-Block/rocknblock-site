footer
    .container.footer
        nav.social-links
            ul.social-links__list
                each link in links.length ? links : ['No values']
                    li.social-links__link
                        a.icon(href=link.href rel="noreferrer" target="_blank")
                            img(src=`../../img/social/icon-footer-${link.name}-mask.svg` alt=link.name)

        .footer-services
            span.s-title Services
            .footer-services-list 
